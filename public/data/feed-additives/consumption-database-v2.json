{
  "database_info": {
    "name": "Animal Consumption Reference Database",
    "version": "2.0",
    "last_updated": "2026-02-09",
    "purpose": "Complete reference data for water and feed consumption calculations",
    "units": {
      "water": "ml/animal/day or L/animal/day",
      "feed": "g/animal/day or kg/animal/day",
      "age_days": "days",
      "age_weeks": "weeks",
      "body_weight": "kg"
    }
  },

  "poultry_commercial": {
    "broiler": {
      "calculation_method": "formula",
      "water_formula": {
        "formula": "5.28 * age_days",
        "unit": "ml/bird/day",
        "temperature_adjustment": {
          "normal": 1.0,
          "hot": 1.08,
          "cold": 0.97
        }
      },
      "feed_formula": {
        "formula": "water / 1.77",
        "ratio": "water:feed = 1.77:1",
        "unit": "g/bird/day"
      },
      "age_range": {
        "min_days": 1,
        "max_days": 56
      }
    },

    "layer": {
      "calculation_method": "phase_table",
