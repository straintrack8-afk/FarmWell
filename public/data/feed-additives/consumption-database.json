{
  "database_info": {
    "name": "Animal Consumption Reference Database",
    "version": "2.0",
    "last_updated": "2026-02-09",
    "purpose": "Complete reference data for water and feed consumption calculations with interpolation support",
    "units": {
      "water": "ml/animal/day or L/animal/day",
      "feed": "g/animal/day or kg/animal/day",
      "age_days": "days",
      "age_weeks": "weeks",
      "body_weight": "kg"
    }
  },

  "poultry_commercial": {
    "broiler": {
      "calculation_method": "formula",
      "water_formula": {
        "formula": "5.28 * age_days",
        "unit": "ml/bird/day",
        "temperature_adjustment": {
          "normal": 1.0,
          "hot": 1.08,
          "cold": 0.97,
          "note": "Hot >27°C, Cold <21°C"
        }
      },
      "feed_formula": {
        "formula": "water / 1.77",
        "ratio": "water:feed = 1.77:1",
        "unit": "g/bird/day"
      },
      "age_range": {
        "min_days": 1,
        "max_days": 56,
        "typical_market": "35-42"
      },
      "source": {
        "primary": "Pesti et al., 1985",
        "secondary": "Cobb Management Guide, Ross Management Guide"
      }
    },
    "layer": {
      "calculation_method": "table_with_interpolation",
      "data_by_week": [
