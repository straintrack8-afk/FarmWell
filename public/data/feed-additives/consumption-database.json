  "poultry_breeding": {
    "layer_breeder": {
      "calculation_method": "phase_based",
      "phases": {
        "growing": {
          "age_weeks_end": 20,
          "method": "use_layer_data",
          "note": "Use commercial layer data for growing phase"
        },
        "production": {
          "age_weeks_start": 21,
          "water_ml": 280,
          "feed_g": 160,
          "note": "Higher consumption due to breeding requirements"
        }
      },
      "source": "Hy-Line W-80 Breeder Guide"
    },
    "broiler_breeder": {
      "calculation_method": "phase_based",
      "phases": {
        "growing": {
          "age_weeks_end": 20,
          "method": "use_layer_data",
          "note": "Use commercial layer data for growing phase"
        },
        "production": {
          "age_weeks_start": 21,
          "water_ml": 300,
          "feed_g": 170,
          "note": "Feed restriction crucial, adjust by body weight"
        }
      },
      "source": "Cobb 500 Broiler Breeder Management Guide"
    },
    "color_breeder": {
      "calculation_method": "phase_based_with_adjustment",
      "phases": {
        "growing": {
          "age_weeks_end": 20,
          "method": "use_layer_data_adjusted",
          "adjustment_factor": 0.85,
          "note": "85% of commercial layer data for growing phase"
        },
        "production": {
          "age_weeks_start": 21,
          "water_ml": 250,
          "feed_g": 140,
          "note": "Brown egg production, moderate consumption"
        }
      },
      "source": "ISA Brown Commercial Management Guide"
    }
  },

