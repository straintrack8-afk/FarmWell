    "color_chicken": {
      "calculation_method": "formula_with_adjustment",
      "base": "broiler",
      "adjustment_factor": 0.70,
      "water_formula": "5.28 * age_days * 0.70",
      "feed_formula": "water / 1.77",
      "note": "Native/village chicken, 70% of broiler consumption"
    }
